<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android">
	<TableRow
	android:id="@+id/layout01">
		<com.chess.genesis.MyImageView
		android:id="@+id/topbar_genesis"
		style="@style/FillWidth"
		android:layout_weight="82"
		android:src="@drawable/topbar_genesis"/>
		<com.chess.genesis.MyImageView
		android:id="@+id/chat"
		style="@style/FillWidth"
		android:layout_weight="408"
		android:src="@drawable/chat"/>
	</TableRow>
	<include
	layout="@layout/include_board_names"
	android:layout_below="@id/layout01"
	style="@style/FillWidth"/>
	<include
	layout="@layout/include_board_layout"
	android:layout_below="@+id/board_names"/>
	<include
	layout="@layout/include_board_nav"
	android:layout_alignParentBottom="true"/>
</RelativeLayout>
