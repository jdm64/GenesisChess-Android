<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android">
	<TableRow
	android:id="@+id/table_01">
		<com.chess.genesis.MyImageView
		android:id="@+id/topbar_genesis"
		style="@style/FillWidth"
		android:layout_weight="82"
		android:src="@drawable/topbar_genesis"/>
		<com.chess.genesis.MyImageView
		android:id="@+id/topbar_plus"
		style="@style/FillWidth"
		android:layout_weight="408"
		android:src="@drawable/topbar_plus"/>
	</TableRow>
	<FrameLayout
	android:id="@+id/frame_01"
	style="@style/FillWidth"
	android:layout_below="@id/table_01">
		<TextView
		android:id="@+id/game_search"
		style="@style/FillWidth"
		android:padding="4dp"
		android:layout_gravity="center"
		android:gravity="center"
		android:textSize="16dp"
		android:text="Searching For Opponents..."/>
	</FrameLayout>
	<com.chess.genesis.SwipeTabs
	android:id="@+id/swipetabs"
	android:layout_below="@id/frame_01"
	style="@style/FillWidth"
	android:background="#ff383838"/>
	<android.support.v4.view.ViewPager
	android:id="@+id/swipe_list"
	android:layout_below="@id/swipetabs"
	android:layout_above="@id/botbar"/>
	<com.chess.genesis.MyImageView
	android:id="@+id/botbar"
	android:layout_alignParentBottom="true"
	android:src="@drawable/botbar"/>
</RelativeLayout>
