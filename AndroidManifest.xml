<?xml version="1.0" encoding="utf-8"?>
<manifest
xmlns:android="http://schemas.android.com/apk/res/android"
package="com.chess.genesis"
android:versionCode="15"
android:versionName="@string/version">
	<uses-sdk android:minSdkVersion="7"/>
	<application
	android:label="@string/app_name"
	android:icon="@drawable/icon"
	android:theme="@style/DefaultTheme">
		<activity
		android:name="MainMenu"
		android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name="GameList" android:launchMode="singleTask"/>
		<activity android:name="RegGame"/>
		<activity android:name="GenGame"/>
		<activity android:name="Login"/>
		<activity android:name="Register"/>
		<activity android:name="Settings"/>
		<activity android:name="SubmitMove" android:theme="@style/NoDimDialog"/>
		<service android:name="GenesisNotifier" android:enabled="true"/>
		<receiver android:name="GenesisAlarm"></receiver>
	</application>
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 
